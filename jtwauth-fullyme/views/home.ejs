<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
 
    <% if (locals.allUser) { %>
 
        <table>
            <thead>
                <th>Full Name</th>
                <th>Email</th>
                <th>Password</th>
                
            </thead>
        
        <tbody>
            
            <% allUser.forEach((alluser) => {%>
                <tr>
                <td><%= alluser.Name %></td>
                <td><%= alluser.Email %></td>
                <td><%= alluser.password %></td>
                
            </tr>
                <% })%>
            
        </tbody>
    </table>
    
    <% } %>
</body>
</html>